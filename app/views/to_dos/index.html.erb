<div class="well main">
<h1>Rails ToDo</h1>
<ul class="list-group">
<%= render 'to_dos/new'%>
<% @todos.each do |todo| %>
    <li class="list-group-item">
        <input type="checkbox">
        <% if todo.completed %>
            <del><%= todo.description %></del>
        <% else %>
            <%= todo.description %>
        <% end %>
    </li>
<% end %>
</ul>
</div>
